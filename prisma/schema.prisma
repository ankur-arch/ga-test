generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Post {
  id        BigInt    @id @unique @default(autoincrement())
  title     String
  views     Int
  createdAt DateTime  @default(now())
  updatedAt DateTime? @updatedAt
}
